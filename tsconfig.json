{
	"extends": "./.svelte-kit/tsconfig.json",
	"esModuleInterop": true,
	"forceConsistentCasingInFileNames": true,
	"resolveJsonModule": true,
	"skipLibCheck": true,
	"sourceMap": true,
	"strict": true,
	"compilerOptions": {
		"lib": ["webworker"],
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$/*": ["./src/*"]
		}
	}
}
